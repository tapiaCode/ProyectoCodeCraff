  <DataSets>
    <DataSet Name="DataSetPedidoCredito">
      <Query>
        <DataSourceName>DataSourcePedidoCredito</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@idPedido">
            <Value>=Parameters!idPedido.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  IdPedido, 
        Fecha_Pedido, 
        Estado_Pedido, 
        ModoDePago, 
        IdCliente, 
        IdEmpleado, 
        Fecha_Credito, 
        ROUND(Monto, 2) as Monto, 
        ROUND(Monto_Interes, 2) as Monto_Interes, 
        Email_Personal, 
        Telefono_Personal, 
        NombreCliente, 
        Carnet_Identidad, 
        Genero, 
        Nombre_Rubro, 
        Nombre_Barrio, 
        NombreEmpleado
FROM VGCredito
WHERE IdPedido = @idPedido	</CommandText>
      </Query>
