  <DataSets>
    <DataSet Name="DataSetPedidoFactura">
      <Query>
        <DataSourceName>DataSourcePedidoFactura</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@idPedidoFactura">
            <Value>=Parameters!idPedidoFactura.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  IdPedido, 
        Fecha_Pedido, 
        Estado_Pedido, 
        ModoDePago, 
        IdCliente, 
        IdEmpleado, 
        ROUND(Monto, 2) as Monto,
        Fecha_Emision, 
        Nit, 
        Codigo_Autenticacion, 
        Email_Personal, 
        Telefono_Personal, 
        NombreCliente, 
        Carnet_Identidad, 
        Genero, 
        Nombre_Rubro, 
        Nombre_Barrio, 
        NombreEmpleado
FROM VGFactura
WHERE IdPedido = @idPedidoFactura	</CommandText>
      </Query>
